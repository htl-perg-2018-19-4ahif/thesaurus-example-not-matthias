{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "indent": [true, "spaces", 4],
        "curly": [true, "as-needed"],
        "object-literal-sort-keys": false,
        "no-consecutive-blank-lines": false,
        "trailing-comma": false,
        "no-console": false,
    },
    "rulesDirectory": []
}